{
  "run_name": "Linear_MNST_auxiliary",
  "run_type": "demo",
  "logging": "logging.INFO",
  "device": "cpu",
  "data_config":
  {
    "dataset": "mnist",
    "concat": true,
    "data_dir": "data/datafiles/mnist",
    "data_path": "data/datafiles/housing_prices/train.csv",
    "split_type": "holdout",
    "splits": {"trainset": 0.7,
